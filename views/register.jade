extends base

block vars
	- var title = "register"
block body
	.container
		h1 Register
		form.new-user(method="post")
			h3 Email
			input(type="email" name="email")
			h3 Username
			input(type="text" name="username")
			h3 Password
			input(type="password" name="password")
			br
			br
			input(type="submit")